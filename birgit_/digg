MY_PATH="`dirname \"$0\"`"

changesIn="${MY_PATH}/changesIn"

for stash in `git stash list --format="%gd"`; do
comp_hash="$($MY_PATH/hash $1)"
	if `$changesIn $stash | grep -q "$1"` ; then
		echo $stash
	fi
done

for commit in `git rev-list origin/master~30...`; do
    if `$changesIn $commit | grep -q "$1"`; then
		echo $commit
	fi
done
